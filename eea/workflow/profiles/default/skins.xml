<?xml version="1.0"?>
<object name="portal_skins"
    meta_type="Plone Skins Tool"
    allow_any="False"
    cookie_persistence="False"
    request_varname="plone_skin">

    <object name="eea_workflow_templates"
        meta_type="Filesystem Directory View"
        directory="workflow/skins/eea_workflow_templates"/>

    <skin-path name="*">
        <layer insert-after="custom" 
            name="eea_workflow_templates"/>
    </skin-path>

</object>
